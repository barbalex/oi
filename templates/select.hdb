<div class="form-group">
    <label class="control-label">{{label}}</label>
    <div class="controls">
        <select id="{{objectId}}{{label}}" class="form-control">
            {{#each valueList}}
                <option value="{{this.value}}" {{this.checked}}>{{this.label}}</option>
            {{/each}}
        </select>
    </div>
</div>
<div class="form-group">
    <label class="control-label">{{object.label}}</label>
    <div class="controls reducedMargin">
        <div class="checkbox">
            <label>
                <input type="checkbox" id="{{object._id}}{{object.label}}" data-object="{{json object}}" value="true" {{checked}}>
            </label>
        </div>
    </div>
</div>
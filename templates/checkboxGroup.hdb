<div class="form-group">
    <label class="control-label">{{label}}</label>
    <div class="controls reducedMargin">
        {{#each valueList}}
            <div class="checkbox">
                <label>
                    <input type="checkbox" id="{{../objectId}}{{../label}}{{@index}}" value="{{this.value}}" {{this.checked}}>
                    {{this.label}}
                </label>
            </div>
        {{/each}}
    </div>
</div>